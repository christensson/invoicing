{
  "name": "Invoicing",
  "version": "0.4.0",
  "description": "Invoicing made easy",
  "main": "app.js",
  "scripts": {
    "test": "mocha --recursive ./test",
    "preversion": "npm test",
    "postversion": "git checkout release && git merge master -m \"Release $npm_package_version\" && git checkout master",
    "prestart": "mkdir -p tmp && mkdir -p log && npm run clean",
    "start": "node .",
    "start-dev": "node . --local",
    "init-db": "node ./script/initdb.js",
    "init-db-data": "node ./script/initdb.js --dev",
    "clean": "rm -rf tmp/*",
    "clean-log": "echo '' > log/log.txt"
  },
  "author": "Marcus Christensson",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "commander": "^2.9.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "csvtojson": "^0.5.14",
    "director": "^1.2.8",
    "express": "^4.14.0",
    "express-enforces-ssl": "^1.1.0",
    "express-session": "^1.14.2",
    "express-simulate-latency": "0.0.2",
    "fluentreports": "^1.1.3",
    "font-awesome": "^4.7.0",
    "helmet": "^1.3.0",
    "i18next": "^2.5.1",
    "i18next-express-middleware": "^0.2.1",
    "i18next-node-fs-backend": "0.0.6",
    "i18next-xhr-backend": "^0.3.0",
    "jquery": "^2.2.3",
    "jqueryui": "^1.11.1",
    "js-cache": "^1.0.2",
    "knockout": "^3.4.1",
    "knockout-bootstrap": "^0.2.0",
    "knockout-jqautocomplete": "^0.4.4",
    "marko": "^3.12.1",
    "method-override": "^2.3.7",
    "mocha": "^3.2.0",
    "mongodb": "^2.2.11",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^0.2.0",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "unit.js": "^2.0.0"
  }
